{
  "manifest_version": 2,
  "name": "Vac-xtension",
  "version": "1.0",
  "permissions": ["https://*/","http://*/","alarms", "storage","tabs","activeTab"],

  "background": {
    "scripts": ["background.js"]
  },
  "icons":{
    "16":"vaccine.png",
    "48":"vaccine.png",
    "128":"vaccine.png"
  },
  "browser_action":{
    "default_icon": "vaccine.png",
    "default_popup": "popup.html"
  }
}