{
  "manifest_version": 3,
  "name": "Vac-xtension",
  "version": "1.0",
  "host_permissions":["<all_urls>"],
  "permissions": ["alarms", "storage","tabs","activeTab"],

  "background": {
    "service_worker": "background.js"
  },
  "action":{
    "default_icon": {              
      "16": "vaccine.png",   
      "128": "vaccine.png",   
      "48": "vaccine.png"    
    },
    "default_popup": "popup.html"
  }
}